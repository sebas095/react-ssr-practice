    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-loadable-visibility": "^3.0.2",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
