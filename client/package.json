{
  "name": "client",
  "version": "1.0.0",
  "description": "MercadoLibre App",
  "main": "index.js",
  "scripts": {
    "build": "ENV=production webpack-cli --config webpack.config.js",
    "build:dll": "webpack --config webpack.dll.config.js",
